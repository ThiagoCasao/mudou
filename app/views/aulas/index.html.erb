<p id="notice"><%= notice %></p>

<h1>Aulas</h1>

<table>
  <thead>
    <tr>
      <th>Disciplina</th>
      <th>Data</th>
      <th>Duracao</th>
      <th>Titulo</th>
      <th>Objetivo</th>
      <th>Ata</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @aulas.each do |aula| %>
      <tr>
        <td><%= aula.disciplina %></td>
        <td><%= aula.data %></td>
        <td><%= aula.duracao %></td>
        <td><%= aula.titulo %></td>
        <td><%= aula.objetivo %></td>
        <td><%= aula.ata %></td>
        <td><%= link_to 'Show', aula %></td>
        <td><%= link_to 'Edit', edit_aula_path(aula) %></td>
        <td><%= link_to 'Destroy', aula, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Aula', new_aula_path %>
